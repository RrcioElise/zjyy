{
  "name": "zj2y_app",
  "version": "1.0.0",
  "author": "Lison<lison.modern@gmail.com>",
  "private": false,
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "build": "rm -rf public/dist && vue-cli-service build --mode prod",
